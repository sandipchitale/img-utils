{
  "name": "Transform",
  "manifest_version": 3,
  "version": "1.0",
  "description": "Utilities for img tag.",
  "permissions": [
    "contextMenus",
    "activeTab",
    "tabs",
    "scripting"
  ],
  "host_permissions": ["<all_urls>"],
  "icons": {
    "256": "images/icon256.png"
  },
  "background": {
    "service_worker": "img_utils_worker.js",
    "type": "module"
  }
}
